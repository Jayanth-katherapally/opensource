import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
 
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int y=sc.nextInt();
        int z=sc.nextInt();
        int min= x*y;
        int min2= z*24*60;
        if(min <= min2)
            System.out.println("YES");
        else
            System.out.println("NO");
        
    }
}
