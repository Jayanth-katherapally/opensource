import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      
         Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a = 1, b = 1;
        for (int i = 3; i <= n; i++) {
            int temp = a + b;
            a = b;
            b = temp;
        }
        System.out.println(b);
    }
}
