import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      
         Scanner sc= new Scanner(System.in);
        int n = sc.nextInt();
        long Sum1 = (long) n * (n + 1) / 2;
        long Sum2 = 0;
        for (int i = 0; i < n - 1; i++) Sum2 += sc.nextInt();
        System.out.println(Sum1 - Sum2);
    }
}
